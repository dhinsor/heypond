{
    "compilerOptions": {
        "target": "ES2022",
        "lib": [
            "ES2022",
            "WebWorker"
        ],
        "module": "ESNext",
        "moduleResolution": "bundler",
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "strict": true,
        "types": [
            "@netlify/edge-functions"
        ]
    },
    "include": [
        "**/*.ts"
    ]
}